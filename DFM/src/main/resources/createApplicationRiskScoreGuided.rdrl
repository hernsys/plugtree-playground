import com.decisionmanagementsolutions.model.PartialApplication;
import com.decisionmanagementsolutions.model.Application;
import java.lang.Double;

rule "createApplicationRiskScoreGuided"
	dialect "mvel"
	when
		$value : Double( ) from accumulate ( PartialApplication( $partial : partialScore),
			count($partial)) 
	then
		Application fact0 = new Application();
		fact0.setRiskScore( $value.intValue() );
		insert( fact0 );
end
